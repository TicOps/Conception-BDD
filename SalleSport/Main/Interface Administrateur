

Ajouter un membre


Valider une Inscription

Modifier un membre

Supprimer une adhésion

Afficher infos d’un membre

Ajouter / modifier / supprimer un cours

Ajouter / gérer les coachs

Gérer les salles

Gestion différente admin principal / secondaire

using System;
using System.Collections.Generic;
using System.Data.SqlClient;
using System.Linq;

namespace Main
{

    // ========== GESTIONNAIRE DE BASE DE DONNÉES ==========
    
    public class DatabaseManager
    {
        private string connectionString;

        public DatabaseManager(string connString)
        {
            connectionString = connString;
        }

        public SqlConnection GetConnection()
        {
            return new SqlConnection(connectionString);
        }
    }

    // ========== SERVICE DE GESTION DES memberS ==========
    
    public class MembreService
    {
        private DatabaseManager dbManager;

        public MembreService(DatabaseManager dbManager)
        {
            this.dbManager = dbManager;
        }

        /// <summary>
        /// Ajoute un nouveau membre dans la base de données
        /// </summary>
        public bool AjouterMembre(Membre membre)
        {
            try
            {
                using (SqlConnection conn = dbManager.GetConnection())
                {
                    conn.Open();
                    string query = @"INSERT INTO Membre (idMembre, nom, prenom, email, mdp,telephone, 
                                   dateDebut, statutInscription, dateFin) 
                                   VALUES (@id, @nom, @prenom, @email, @tel, @date, @statut, @dateFin)";

                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    {
                        cmd.Parameters.AddWithValue("@id", membre.IdMembre);
                        cmd.Parameters.AddWithValue("@nom", membre.Nom);
                        cmd.Parameters.AddWithValue("@prenom", membre.Prenom);
                        cmd.Parameters.AddWithValue("@email", membre.Email);
                        cmd.Parameters.AddWithValue("@mdp", membre.Mdp);
                        cmd.Parameters.AddWithValue("@tel", membre.Telephone);
                        cmd.Parameters.AddWithValue("@dateDebut", membre.DateDebut);
                        cmd.Parameters.AddWithValue("@statut", "En attente");
                        cmd.Parameters.AddWithValue("@dateFin", membre.DateFin);

                        int result = cmd.ExecuteNonQuery();
                        return result > 0;
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur lors de l'ajout du membre : {ex.Message}");
                return false;
            }
        }

        /// <summary>
        /// Valide l'Inscription d'un membre en attente
        /// </summary>
        public bool ValiderInscription(string idMembre)
        {
            try
            {
                using (SqlConnection conn = dbManager.GetConnection())
                {
                    conn.Open();
                    string query = @"UPDATE Membre 
                                   SET statutInsciprtion = 'Validée', 
                                       dateDebut = @dateDebut 
                                   WHERE idMembre = @idMembre AND statutInscription = 'En attente'";

                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    {
                        cmd.Parameters.AddWithValue("@id", idMembre);
                        cmd.Parameters.AddWithValue("@dateValidation", DateTime.Now);

                        int result = cmd.ExecuteNonQuery();
                        return result > 0;
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur lors de la validation : {ex.Message}");
                return false;
            }
        }

        /// <summary>
        /// Modifie les informations d'un membre existant
        /// </summary>
        public bool ModifierMembre(Membre membre)
        {
            try
            {
                using (SqlConnection conn = dbManager.GetConnection())
                {
                    conn.Open();
                    string query = @"UPDATE Membre 
                                   SET nom = @nom, prenom = @prenom, email = @email, 
                                       telephone = @tel, dateExpiration = @dateExp 
                                   WHERE idMembre = @idMembre";

                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    {
                        cmd.Parameters.AddWithValue("@idMembre", membre.IdMembre);
                        cmd.Parameters.AddWithValue("@nom", membre.Nom);
                        cmd.Parameters.AddWithValue("@prenom", membre.Prenom);
                        cmd.Parameters.AddWithValue("@email", membre.Email);
                        cmd.Parameters.AddWithValue("@tel", membre.Telephone);
                        cmd.Parameters.AddWithValue("@dateExp", membre.DateExpiration);

                        int result = cmd.ExecuteNonQuery();
                        return result > 0;
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur lors de la modification : {ex.Message}");
                return false;
            }
        }

        /// <summary>
        /// Supprime l'adhésion d'un membre (soft delete)
        /// </summary>
        public bool SupprimerAdhesion(string idMembre)
        {
            try
            {
                using (SqlConnection conn = dbManager.GetConnection())
                {
                    conn.Open();
                    string query = @"UPDATE Membre 
                                   SET statutInscription = 'Expirée', 
                                       dateExpiration = @dateFin 
                                   WHERE idMembre = @idMembre";

                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    {
                        cmd.Parameters.AddWithValue("@id", idMembre);
                        cmd.Parameters.AddWithValue("@dateFin", DateTime.Now);

                        int result = cmd.ExecuteNonQuery();
                        return result > 0;
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur lors de la suppression : {ex.Message}");
                return false;
            }
        }

        /// <summary>
        /// Récupère et affiche les informations détaillées d'un membre
        /// </summary>
        public Membre ObtenirInfosMembre(string idMembre)
        {
            try
            {
                using (SqlConnection conn = dbManager.GetConnection())
                {
                    conn.Open();
                    string query = @"SELECT * FROM Membre WHERE idMembre = @idMembre";

                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    {
                        cmd.Parameters.AddWithValue("@idMembre", idMembre);

                        using (SqlDataReader reader = cmd.ExecuteReader())
                        {
                            if (reader.Read())
                            {
                                return new Membre
                                {
                                    IdMembre = reader["idMembre"].ToString(),
                                    Nom = reader["nom"].ToString(),
                                    Prenom = reader["prenom"].ToString(),
                                    Email = reader["email"].ToString(),
                                    Telephone = reader["telephone"].ToString(),
                                    DateDebut = Convert.ToDateTime(reader["dateDebut"]),
                                    StatutInscription = reader["statutInscription"].ToString(),
                                    DateExpiration = Convert.ToDateTime(reader["dateFin"])
                                };
                            }
                        }
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur lors de la récupération : {ex.Message}");
            }
            return null;
        }

        /// <summary>
        /// Liste tous les membres avec leur statut
        /// </summary>
        public List<Membre> ListerMembres(string filtreStatut = null)
        {
            List<Membre> membres = new List<Membre>();
            try
            {
                using (SqlConnection conn = dbManager.GetConnection())
                {
                    conn.Open();
                    string query = filtreStatut == null 
                        ? "SELECT * FROM Membre ORDER BY nom, prenom"
                        : "SELECT * FROM Membre WHERE statutInscription = @statutInscription ORDER BY nom, prenom";

                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    {
                        if (filtreStatut != null)
                            cmd.Parameters.AddWithValue("@statutInscriptio ", filtreStatut);

                        using (SqlDataReader reader = cmd.ExecuteReader())
                        {
                            while (reader.Read())
                            {
                                membres.Add(new Membre
                                {
                                    IdMembre = reader["idMembre"].ToString(),
                                    Nom = reader["nom"].ToString(),
                                    Prenom = reader["prenom"].ToString(),
                                    Email = reader["email"].ToString(),
                                    Telephone = reader["telephone"].ToString(),
                                    DateDebut = Convert.ToDateTime(reader["dateDebut"]),
                                    StatutInscription = reader["statutInscription"].ToString(),
                                    DateExpiration = Convert.ToDateTime(reader["dateFin"])
                                });
                            }
                        }
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur lors du listage : {ex.Message}");
            }
            return membres;
        }
    }

    // ========== SERVICE DE GESTION DES COACHS ==========
    
    public class CoachService
    {
        private DatabaseManager dbManager;

        public CoachService(DatabaseManager dbManager)
        {
            this.dbManager = dbManager;
        }

        /// <summary>
        /// Ajoute un nouveau coach
        /// </summary>
        public bool AjouterCoach(Coach coach)
        {
            try
            {
                using (SqlConnection conn = dbManager.GetConnection())
                {
                    conn.Open();
                    string query = @"INSERT INTO Coach (idCoach, nom, prenom, specialite, email, telephone, estActif) 
                                   VALUES (@idCoach, @nom, @prenom, @specialite, @email, @tel, @actif)";

                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    {
                        cmd.Parameters.AddWithValue("@idCoach", coach.IdCoach);
                        cmd.Parameters.AddWithValue("@nom", coach.Nom);
                        cmd.Parameters.AddWithValue("@prenom", coach.Prenom);
                        cmd.Parameters.AddWithValue("@specialite", coach.Specialite);
                        cmd.Parameters.AddWithValue("@email", coach.Email);
                        cmd.Parameters.AddWithValue("@tel", coach.Telephone);
                        cmd.Parameters.AddWithValue("@actif", true);

                        int result = cmd.ExecuteNonQuery();
                        return result > 0;
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur lors de l'ajout du coach : {ex.Message}");
                return false;
            }
        }

        /// <summary>
        /// Modifie les informations d'un coach
        /// </summary>
        public bool ModifierCoach(Coach coach)
        {
            try
            {
                using (SqlConnection conn = dbManager.GetConnection())
                {
                    conn.Open();
                    string query = @"UPDATE Coach 
                                   SET nom = @nom, prenom = @prenom, specialite = @specialite, 
                                       email = @email, estActif = @estActif 
                                   WHERE idCoach = @idCoach";

                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    {
                        cmd.Parameters.AddWithValue("@id", coach.IdCoach);
                        cmd.Parameters.AddWithValue("@nom", coach.Nom);
                        cmd.Parameters.AddWithValue("@prenom", coach.Prenom);
                        cmd.Parameters.AddWithValue("@specialite", coach.Specialite);
                        cmd.Parameters.AddWithValue("@email", coach.Email);
                        cmd.Parameters.AddWithValue("@estActif", coach.EstActif);

                        int result = cmd.ExecuteNonQuery();
                        return result > 0;
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur lors de la modification du coach : {ex.Message}");
                return false;
            }
        }

        /// <summary>
        /// Désactive un coach (soft delete)
        /// </summary>
        public bool SupprimerCoach(string idCoach)
        {
            try
            {
                using (SqlConnection conn = dbManager.GetConnection())
                {
                    conn.Open();
                    string query = "UPDATE Coach SET estActif = 0 WHERE idCoach = @idCoach";

                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    {
                        cmd.Parameters.AddWithValue("@idCoach", idCoach);
                        int result = cmd.ExecuteNonQuery();
                        return result > 0;
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur lors de la suppression du coach : {ex.Message}");
                return false;
            }
        }

        /// <summary>
        /// Liste tous les coachs actifs
        /// </summary>
        public List<Coach> ListerCoachs(bool uniquementActifs = true)
        {
            List<Coach> coachs = new List<Coach>();
            try
            {
                using (SqlConnection conn = dbManager.GetConnection())
                {
                    conn.Open();
                    string query = uniquementActifs 
                        ? "SELECT * FROM Coach WHERE estActif = 1 ORDER BY nom, prenom"
                        : "SELECT * FROM Coach ORDER BY nom, prenom";

                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    using (SqlDataReader reader = cmd.ExecuteReader())
                    {
                        while (reader.Read())
                        {
                            coachs.Add(new Coach
                            {
                                IdCoach = reader["idCoach"].ToString(),
                                Nom = reader["nom"].ToString(),
                                Prenom = reader["prenom"].ToString(),
                                Specialite = reader["specialite"].ToString(),
                                Email = reader["email"].ToString(),
                                Telephone = reader["telephone"].ToString(),
                                EstActif = Convert.ToBoolean(reader["estActif"])
                            });
                        }
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur lors du listage des coachs : {ex.Message}");
            }
            return coachs;
        }
    }

    // ========== SERVICE DE GESTION DES COURS ==========
    
    public class CoursService
    {
        private DatabaseManager dbManager;

        public CoursService(DatabaseManager dbManager)
        {
            this.dbManager = dbManager;
        }

        /// <summary>
        /// Ajoute un nouveau cours
        /// </summary>
        public bool AjouterCours(Cours cours)
        {
            try
            {
                using (SqlConnection conn = dbManager.GetConnection())
                {
                    conn.Open();
                    string query = @"INSERT INTO Cours (idCours, idCoach, 
                                   idSalle, capaciteMax, duree) 
                                   VALUES (@idCours, @idCoach, @salle, @capaciteMax, @nomSalle)";

                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    {
                        cmd.Parameters.AddWithValue("@idCours", cours.IdCours);
                        cmd.Parameters.AddWithValue("@idCoach", cours.IdCoach);
                        cmd.Parameters.AddWithValue("@idSalle", cours.IdSalle);
                        cmd.Parameters.AddWithValue("@capaciteMax", cours.CapaciteMax);
                        cmd.Parameters.AddWithValue("@nomSalle", cours.nomSalle);

                        int result = cmd.ExecuteNonQuery();
                        return result > 0;
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur lors de l'ajout du cours : {ex.Message}");
                return false;
            }
        }

        /// <summary>
        /// Modifie un cours existant
        /// </summary>
        public bool ModifierCours(Cours cours)
        {
            try
            {
                using (SqlConnection conn = dbManager.GetConnection())
                {
                    conn.Open();
                    string query = @"UPDATE Cours 
                                   SET  idCoach = @idCoach, nomSalle = @nomSalle, capaciteMax = @capaciteMax, nomCours = @nomCours 
                                   WHERE idCours = @idCours";

                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    {
                        cmd.Parameters.AddWithValue("@idCours", cours.IdCours);
                        cmd.Parameters.AddWithValue("@idCoach", cours.IdCoach);
                        cmd.Parameters.AddWithValue("@idSalle", cours.IdSalle);
                        cmd.Parameters.AddWithValue("@capaciteMax", cours.CapaciteMax);
                        cmd.Parameters.AddWithValue("@nomSalle", cours.nomSalle);
                        cmd.Parameters.AddWithValue("@nomCours", cours.nomCours);

                        int result = cmd.ExecuteNonQuery();
                        return result > 0;
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur lors de la modification du cours : {ex.Message}");
                return false;
            }
        }

        /// <summary>
        /// Supprime un cours
        /// </summary>
        public bool SupprimerCours(string idCours)
        {
            try
            {
                using (SqlConnection conn = dbManager.GetConnection())
                {
                    conn.Open();
                    string query = "DELETE FROM Cours WHERE idCours = @idCours";

                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    {
                        cmd.Parameters.AddWithValue("@idCours", idCours);
                        int result = cmd.ExecuteNonQuery();
                        return result > 0;
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur lors de la suppression du cours : {ex.Message}");
                return false;
            }
        }

        /// <summary>
        /// Liste tous les cours
        /// </summary>
        public List<Cours> ListerCours()
        {
            List<Cours> cours = new List<Cours>();
            try
            {
                using (SqlConnection conn = dbManager.GetConnection())
                {
                    conn.Open();
                    string query = "SELECT * FROM Cours ORDER BY horaire";

                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    using (SqlDataReader reader = cmd.ExecuteReader())
                    {
                        while (reader.Read())
                        {
                            cours.Add(new Cours
                            {
                                IdCours = reader["idCours"].ToString(),
                                NomCours = reader["nomCours"].ToString(),
                                Description = reader["description"].ToString(),
                                IdCoach = reader["idCoach"].ToString(),
                                IdSalle = reader["idSalle"].ToString(),
                                Horaire = Convert.ToDateTime(reader["horaire"]),
                                CapaciteMax = Convert.ToInt32(reader["capaciteMax"]),
                                Duree = Convert.ToInt32(reader["duree"])
                            });
                        }
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur lors du listage des cours : {ex.Message}");
            }
            return cours;
        }
    }

    // ========== SERVICE DE GESTION DES SALLES ==========
    
    public class SalleService
    {
        private DatabaseManager dbManager;

        public SalleService(DatabaseManager dbManager)
        {
            this.dbManager = dbManager;
        }

        /// <summary>
        /// Ajoute une nouvelle salle
        /// </summary>
        public bool AjouterSalle(Salle salle)
        {
            try
            {
                using (SqlConnection conn = dbManager.GetConnection())
                {
                    conn.Open();
                    string query = @"DebutERT INTO Salle (idSalle, nomSalle, capacite, equipements, estDisponible) 
                                   VALUES (@id, @nom, @cap, @equip, @dispo)";

                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    {
                        cmd.Parameters.AddWithValue("@id", salle.IdSalle);
                        cmd.Parameters.AddWithValue("@nom", salle.NomSalle);
                        cmd.Parameters.AddWithValue("@cap", salle.Capacite);
                        cmd.Parameters.AddWithValue("@equip", salle.Equipements);
                        cmd.Parameters.AddWithValue("@dispo", true);

                        int result = cmd.ExecuteNonQuery();
                        return result > 0;
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur lors de l'ajout de la salle : {ex.Message}");
                return false;
            }
        }

        /// <summary>
        /// Modifie une salle existante
        /// </summary>
        public bool ModifierSalle(Salle salle)
        {
            try
            {
                using (SqlConnection conn = dbManager.GetConnection())
                {
                    conn.Open();
                    string query = @"UPDATE Salle 
                                   SET nomSalle = @nom, capaciteMax = @capaciteMax, 
                                       estDisponible = @estDisponible
                                   WHERE idSalle = @id";

                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    {
                        cmd.Parameters.AddWithValue("@id", salle.IdSalle);
                        cmd.Parameters.AddWithValue("@nom", salle.NomSalle);
                        cmd.Parameters.AddWithValue("@capaciteMax", salle.Capacite);
                        cmd.Parameters.AddWithValue("@estDisponible", salle.EstDisponible);

                        int result = cmd.ExecuteNonQuery();
                        return result > 0;
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur lors de la modification de la salle : {ex.Message}");
                return false;
            }
        }

        /// <summary>
        /// Supprime une salle
        /// </summary>
        public bool SupprimerSalle(string idSalle)
        {
            try
            {
                using (SqlConnection conn = dbManager.GetConnection())
                {
                    conn.Open();
                    string query = "UPDATE Salle SET estDisponible = 0 WHERE idSalle = @idSalle";

                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    {
                        cmd.Parameters.AddWithValue("@idSalle", idSalle);
                        int result = cmd.ExecuteNonQuery();
                        return result > 0;
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur lors de la suppression de la salle : {ex.Message}");
                return false;
            }
        }

        /// <summary>
        /// Liste toutes les salles
        /// </summary>
        public List<Salle> ListerSalles(bool uniquementDisponibles = false)
        {
            List<Salle> salles = new List<Salle>();
            try
            {
                using (SqlConnection conn = dbManager.GetConnection())
                {
                    conn.Open();
                    string query = uniquementDisponibles 
                        ? "SELECT * FROM Salle WHERE estDisponible = 1 ORDER BY nomSalle"
                        : "SELECT * FROM Salle ORDER BY nomSalle";

                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    using (SqlDataReader reader = cmd.ExecuteReader())
                    {
                        while (reader.Read())
                        {
                            salles.Add(new Salle
                            {
                                IdSalle = reader["idSalle"].ToString(),
                                NomSalle = reader["nomSalle"].ToString(),
                                Capacite = Convert.ToInt32(reader["capacite"]),
                                EstDisponible = Convert.ToBoolean(reader["estDisponible"])
                            });
                        }
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur lors du listage des salles : {ex.Message}");
            }
            return salles;
        }
    }

    // ========== SERVICE DE CONTRÔLE D'ACCÈS ==========
    
    public class AutorisationService
    {
        /// <summary>
        /// Vérifie si un administrateur a les privilèges requis
        /// </summary>
        public bool VerifierPrivilege(string idGerant, string actionRequise)
        {
            // Actions réservées à l'administrateur principal
            string[] actionsPrincipales = { 
                "SUPPRIMER_MEMBRE", 
                "SUPPRIMER_COACH", 
                "GERER_ADMINISTRATEURS",
                "ACCES_COMPLET_STATS"
            };

            try
            {
                using (SqlConnection conn = new SqlConnection("VotreConnectionString"))
                {
                    conn.Open();
                    
                    // Vérifier d'abord dans Administrateur_1 (principal)
                    string query = "SELECT niveauPrivliege FROM Administrateur_1 WHERE idGerant = @idGerant";
                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    {
                        cmd.Parameters.AddWithValue("@idGerant", idGerant);
                        object result = cmd.ExecuteScalar();
                        
                        if (result != null)
                            return true; // Admin principal a tous les droits
                    }

                    // Sinon vérifier dans Administrateur_2 (secondaire)
                    query = "SELECT niveauPrivliege FROM Administrateur_2 WHERE idGerant = @idGerant";
                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    {
                        cmd.Parameters.AddWithValue("@id", idGerant);
                        object result = cmd.ExecuteScalar();
                        
                        if (result != null)
                        {
                            // Admin secondaire ne peut pas effectuer certaines actions
                            return !actionsPrincipales.ContaDebut(actionRequise);
                        }
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur de vérification des privilèges : {ex.Message}");
            }

            return false;
        }

        /// <summary>
        /// Obtient le niveau de privilège d'un administrateur
        /// </summary>
        public string ObtenirNiveauPrivilege(string idGerant)
        {
            try
            {
                using (SqlConnection conn = new SqlConnection("VotreConnectionString"))
                {
                    conn.Open();
                    
                    string query = "SELECT niveauPrivliege FROM Administrateur_1 WHERE idGerant = @idGerant";
                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    {
                        cmd.Parameters.AddWithValue("@id", idGerant);
                        object result = cmd.ExecuteScalar();
                        if (result != null) return "Principal";
                    }

                    query = "SELECT niveauPrivliege FROM Administrateur_2 WHERE idGerant = @idGerant";
                    using (SqlCommand cmd = new SqlCommand(query, conn))
                    {
                        cmd.Parameters.AddWithValue("@idGerant", idGerant);
                        object result = cmd.ExecuteScalar();
                        if (result != null) return "Secondaire";
                    }
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine($"Erreur : {ex.Message}");
            }
            return "Inconnu";
        }
    }
}       

